<%
	var result = data.result;
	var rows = [];
%>
<div class="container">
	<div class="row">
		<h2 class="title">Results for <%=data.baseUrl%></h2>
		<h4 class="title">(<%=Object.keys(result).length%> URLs found and tested)</h4>
	</div>
</div>

<table id="results" class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>URL Provided to Server Component</th>
			<th>Response From <%=data.scUrl1%></th>
			<th>Response From <%=data.scUrl2%></th>
		</tr>
	</thead>
	<tbody>
		<% Object.keys(result).forEach(function(key) {
				var val = result[key];
		%>
		<tr id="<%= result.id %>">
			<td><%=key.replace(data.baseUrl, '')%></td>
			<td><%=val[data.scUrl1]%></td>
			<td><%=val[data.scUrl2]%></td>
		</tr>
		<% }); %>
	</tbody>
</table>
